      },
      "ReimbursementContext": {
        "reimbursementRequestID": "$nav.sak.kontekst.refusjonskrav.id",
        "totalNumberIndividualClaims": "$nav.sak.kontekst.refusjonskrav.antallkrav"
      }
    },
    "RemovedParticipant": {
      "Institution": {
        "institutionID": "$nav.sak.fjerninstitusjon.institusjon.id",
        "institutionName": "$nav.sak.fjerninstitusjon.institusjon.navn"
      },
      "reason": {
        "value": [
          "$nav.sak.fjerninstitusjon.grunn.type"
        ]
      },
      "pleaseProvideMoreDetailsIf99OtherSelected": "$nav.sak.fjerninstitusjon.grunn.annet"
